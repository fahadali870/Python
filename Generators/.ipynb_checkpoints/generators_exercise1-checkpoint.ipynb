   "source": [
    "def square_generator():\n",
    "    num = 1  \n",
    "    while True:  \n",
    "        yield num * num  \n",
    "        num += 1  \n",
    "\n",
    "sq_gen = square_generator()\n",
    "\n",
    "for _ in range(10):  \n",
    "    print(next(sq_gen))\n"
   ]
  }
